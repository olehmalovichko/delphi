   MEMVAR->SUM_IT := 0
   MEMVAR->SUM_IT_K := 0
   MEMVAR->KWO_IT := 0
   MEMVAR->KWO_K := 0
   MEMVAR->KWO_N := 0
   MEMVAR->NUM := 0
