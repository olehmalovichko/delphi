   MEMVAR->SUM_N := 0
   MEMVAR->SUM_K_K := 0
   MEMVAR->SUM_K_N := 0
   LEV := 0
   DBSELECTAR("ef_jpkpl")

   IF DBSEEK(DTOS(DAT_OD__))

      SET(24, ("") + FNAME, .F.)
      SET(20, "PRINTER")
      RW := PROW()
      DEVPOS(RW + 0, 0)
      DEVOUT(CHR(27)+"9")
      DEVPOS(RW + 0, 0)
      DEVOUT(CHR(27)+"@")
      DEVPOS(RW + 0, 0)
      DEVOUT(CHR(27)+"x" + CHR(0))
      DEVPOS(RW + 0, 0)
      DEVOUT(CHR(27)+"!"+CHR(4))
      DEVPOS(RW + 0, 0)
      DEVOUT(CHR(27)+"3"+CHR(28))
      DEVPOS(RW++, 13)
      DEVOUT("                       Вiдомiсть прийнятих платежiв  " + IF(;
            KN_SYS->PR_PK, "вечерняя касса", " "))
      DEVPOS(RW++, 5)
      DEVOUT("            " + DTOC(DAT_OD__) + ;
            " р.                      (комунальнi  платежi)")
      DEVPOS(RW++, 0)
      DEVOUT(;
            "==============================================================================================================================";
            )
      DEVPOS(RW++, 0)
      DEVOUT(;
            "!       Одержувач         ! М Ф О!   Рахунок    !      Сума      ! Вид !Одерж. комiс.!  Прiзвище I. П.    !      О/рахунок   !";
            )
      DEVPOS(RW++, 0)
      DEVOUT(;
            "!                         !      !              !                !опл. ! з платника  !    платника        !      О/рахунок   !";
            )
      DEVPOS(RW++, 0)
      DEVOUT(;
            "!============================================================================================================================!";
            )

      DO WHILE EF_JPKPL->DAT_BANK == DAT_OD__ .AND. .NOT. EOF()

         IF LEV = 0

            MEMVAR->SUM_T := 0
            MEMVAR->SUM_T_K := 0
            MEMVAR->KWO_T := 0
            ++LEV
            MEMVAR->KOD_T := EF_JPKPL->KOD_VPL
            EF_S_GPL->(DBSEEK(STR(KOD_T, 3)))
            DEVPOS(RW++, 0)
            DEVOUT("!                                        " + EF_S_GPL->;
                  NAM_VPL + ;
                  "                                                                !";
                  )

         ENDIF

         EF_S_PPL->(DBSEEK(EF_JPKPL->KOD_POL))
         MEMVAR->PPP := STR(EF_S_PPL->SC, 14)

         IF EF_JPKPL->PR_PK = KN_SYS->PR_PK

            MEMVAR->SUMM := EF_JPKPL->SUMMA
            MEMVAR->SUMK := EF_JPKPL->SUM_KOM
            DEVPOS(RW++, 0)
            DEVOUT("!" + SUBSTR(EF_S_PPL->NAM_POL, 1, 25) + "!" + STR(;
                  EF_S_PPL->MFO, 6) + "!" + PPP + "!" + STR(SUMM, 16, 2) + ;
                  "!" + IF(EF_JPKPL->PR_N_K, "карта", "нал. ") + "!" + IF(;
                  EF_JPKPL->PR_TAR, SPACE(13), STR(SUMK, 13, 2)) + "!" + ;
                  SUBSTR(EF_JPKPL->NAM_PLAT, 1, 20) + "!" + EF_JPKPL->;
                  LS_PLAT + "!")
            MEMVAR->SUM_IT_K := MEMVAR->SUM_IT_K + IF(EF_JPKPL->PR_TAR, 0.00, ;
                  SUMK)
            MEMVAR->SUM_T_K := MEMVAR->SUM_T_K + IF(EF_JPKPL->PR_TAR, 0.00, ;
                  SUMK)
            MEMVAR->SUM_IT := MEMVAR->SUM_IT + SUMM
            MEMVAR->SUM_T := MEMVAR->SUM_T + SUMM

            IF EF_JPKPL->PR_N_K

               MEMVAR->SUM_K := MEMVAR->SUM_K + SUMM
               MEMVAR->SUM_K_K := MEMVAR->SUM_K_K + IF(EF_JPKPL->PR_TAR, 0.00,;
                      SUMK)
               ++MEMVAR->KWO_K

            ELSE

               MEMVAR->SUM_N := MEMVAR->SUM_N + SUMM
               MEMVAR->SUM_K_N := MEMVAR->SUM_K_N + IF(EF_JPKPL->PR_TAR, 0.00,;
                      SUMK)
               ++MEMVAR->KWO_N

            ENDIF

            ++MEMVAR->KWO_IT
            ++MEMVAR->KWO_T

         ENDIF

         DBSKIP()
         SET(20, "SCREEN")
         MEMVAR->LIN := INOTDEAD(LIN)
         SET(20, "PRINTER")

         IF (EOF() .OR. EF_JPKPL->KOD_VPL <> KOD_T) .AND. LEV > 0

            LEV := 0
            DEVPOS(RW++, 0)
            DEVOUT("! Разом по " + EF_S_GPL->NAM_VPL + ":   док.: " + STR(;
                  KWO_T, 5) + "   " + STR(SUM_T, 16, 2) + "    " + STR(;
                  SUM_T_K, 16, 2) + ;
                  "                                        !")
            DEVPOS(RW++, 0)
            DEVOUT(;
                  "!----------------------------------------------------------------------------------------------------------------------------!";
                  )

         ENDIF

      ENDDO

      DEVPOS(RW++, 0)
      DEVOUT("!   Разом по вiдомостi:    документiв:   " + STR(KWO_IT, 5) +;
             "   " + STR(SUM_IT, 16, 2) + "    " + STR(SUM_IT_K, 16, 2) + ;
            "                                        !")
      DEVPOS(RW++, 0)
      DEVOUT("!   В.т.р                  готiвкою  :   " + STR(KWO_N, 5) + ;
            "   " + STR(SUM_N, 16, 2) + "    " + STR(SUM_K_N, 16, 2) + ;
            "                                        !")
      DEVPOS(RW++, 0)
      DEVOUT("!                          карткою   :   " + STR(KWO_K, 5) + ;
            "   " + STR(SUM_K, 16, 2) + "    " + STR(SUM_K_K, 16, 2) + ;
            "                                        !")
      DEVPOS(RW++, 0)
      DEVOUT(;
            "!============================================================================================================================!";
            )
      RW := RW + 3
      DEVPOS(RW++, 13)
      DEVOUT(" Бухгалтер     _______________")
      DEVPOS(RW++, 1)
      DEVOUT("")
      __EJECT()
      SET(24, "")
      SET(20, "SCREEN")

   ENDIF

   WINCLOSE(WIN)
   SETCOLOR(OLDC)

   IF FILE(("") + FNAME)

