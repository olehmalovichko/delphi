            FILENAMES(FNAME) + " ", {||PRINTPVD()}, {"~Esc-~выход", ;
            "~F2-~печать"})

   ENDIF

   DBSELECTAR("ef_jpkpl")
   DBGOTO(REC)

RETURN .T.

