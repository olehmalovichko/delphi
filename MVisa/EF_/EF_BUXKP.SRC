/* ------ Rescue5 1.00(d) (c) APTware 1993,94 ------
     Name: OBRIO_1
   Params: None.
   Return: Logical
  Example: OBRIO_1()
  ..........................................................................
*/

STATIC FUNCTION OBRIO_1()

   LOCAL LMORE := .T.
   LOCAL NKEY, PPP, NEW_REC, V_ID_KL, DAT_END

   DISPBEGIN()

   DO WHILE LMORE

      NKEY := 0

      DO WHILE NKEY == 0 .AND. .NOT. BROWSE:STABLE()

         BROWSE:STABILIZE()
         NKEY := INKEY()

      ENDDO

      DISPEND()
      IF(VALTYPE(VYVBLOCK) == "B", EVAL(VYVBLOCK), NIL)

      IF BROWSE:STABLE()

         IF BROWSE:HITTOP() .OR. BROWSE:HITBOTTOM()

            BROWSE:HITTOP := .F.
            BROWSE:HITBOTTOM := .F.

         ENDIF

         NKEY := INKEY(3)

         IF NKEY = 0

            DISPBEGIN()
            BROWSE:REFRESHALL()
            LOOP

         ENDIF

      ENDIF

      DO CASE

      CASE NKEY == 27

         LMORE := .F.
         MEMVAR->LONG := .F.

      CASE NKEY == 24

         BROWSE:DOWN()

      CASE NKEY == 5

         BROWSE:UP()

      CASE NKEY == 1

         IF( .NOT. DBEMPTY(), BROWSE:GOTOP(), "")

      CASE NKEY == 6

         IF( .NOT. DBEMPTY(), BROWSE:GOBOTTOM(), "")

      CASE NKEY == 3

         BROWSE:PAGEDOWN()

      CASE NKEY == 18

         BROWSE:PAGEUP()

      OTHERWISE

         DO WHILE  .NOT. BROWSE:STABILIZE()

         ENDDO

         IF(VALTYPE(VYVBLOCK) == "B", EVAL(VYVBLOCK), NIL)

         IF NKEY = -39

            NKEY := MENU_POPUP(MS1_POPUP, "Меню операций")
            IF(EMPTY(NKEY), NIL, NKEY := MEMVAR->MS3_POPUP[NKEY])

         ENDIF

         IF NKEY == -5

            IF MEMVAR->MS1_POPUP[1, 2]

               V_P_RCH(.F.)

            ENDIF

         ENDIF

         IF NKEY == -6

            IF MEMVAR->MS1_POPUP[2, 2]

               REESTR_KOM()

            ENDIF

         ENDIF

         IF NKEY == -7

            IF MEMVAR->MS1_POPUP[3, 2]

               IT_P_ORD()

            ENDIF

         ENDIF

         IF NKEY == -8 .AND. MEMVAR->MS1_POPUP[4, 2]

            IT_TAR()

         ENDIF

      END CASE

      DISPBEGIN()

   ENDDO

   DISPEND()

RETURN .T.

/* ------ Rescue5 1.00(d) (c) APTware 1993,94 ------
     Name: IT_P_ORD
   Params: None.
   Return: Variable
  Example: IT_P_ORD()
  ..........................................................................
*/

STATIC FUNCTION IT_P_ORD()

   LOCAL KEY := 0, S_USD := 0, S_UAH := 0, I, REC := RECNO()
   LOCAL ARR_KP := {}

   REC := RECNO()

   IF EF_JPKPL->(DBSEEK(DTOS(DAT_OD__)))

      DO WHILE EF_JPKPL->DAT_BANK = DAT_OD__ .AND. .NOT. EF_JPKPL->(EOF())

         IF EF_JPKPL->PR_PK = IF(PR_BA__, .F., KN_SYS->PR_PK) .AND. .NOT. ;
               EF_JPKPL->PR_N_K

            S_UAH := S_UAH + EF_JPKPL->SUMMA
            EF_S_GPL->(DBSEEK(STR(EF_JPKPL->KOD_VPL, 3)))
            MEMVAR->MMM := ASCAN(ARR_KP, {|X|X[1] == EF_S_GPL->CH_KPLAN})

            IF MMM > 0

               ++ARR_KP[MMM, 2]
               ARR_KP[MMM, 3] := ARR_KP[MMM, 3] + EF_JPKPL->SUMMA

            ELSE

               AADD(ARR_KP, {EF_S_GPL->CH_KPLAN, 1, EF_JPKPL->SUMMA})

            ENDIF

         ENDIF

         EF_JPKPL->(DBSKIP())

      ENDDO

      KEY := DIALOG(NIL, NIL, {"@Сформировать", ;
            "сводный приходный кассовый ордер ?"}, {" Да ", " Нет "}, 2)

      IF KEY = 0 .OR. KEY = 2

         RETURN .T.

      ELSEIF KEY = 1

         EF_KASPI(S_UAH, KOD_VUAH__, EF_SYS->NIPO_UAH, ARR_KP)
         DBSELECTAR("ef_jpkpl")
         DBGOTO(REC)

      ENDIF

   ELSE

      ERROR(NIL, NIL, {"@Внимание !", DTOC(DAT_OD__) + " г.", ;
            "не было приема коммунальных плат. !"}, {" Выход "})

   ENDIF

   DBGOTO(REC)

RETURN NIL

/* ------ Rescue5 1.00(d) (c) APTware 1993,94 ------
     Name: IT_TAR
   Params: None.
   Return: Logical
  Example: IT_TAR()
  ..........................................................................
*/

STATIC FUNCTION IT_TAR()

   LOCAL REC := RECNO(), KEY := 0, LEV := 0, OLDORD := EF_S_PPL->(INDEXORD();
         )
   LOCAL KWO_IT := 0, SUM_IT := 0, KOD_T := 0, DAT_, UD_IT := 0, ;
         KOD_V := ""
   LOCAL KWO_T := 0, SUM_T := 0, SCHET_V, OTD_T, UD_MFO := 0
   LOCAL PR_P_S, KWO_SC := 0, SUM_SC := 0, SC_T := 0, UD_SC := 0
   LOCAL NAM_S, NAM_P, KWO_MFO := 0, SUM_MFO := 0, MFO_T := 0, ;
         UD_T := 0, N_D_M := 1000
   LOCAL ARR_ := {}

   IF EF_SYS->FL_VUD

      KEY := ERROR(NIL, NIL, {"@Внимание !", ;
            "Ведомость удержаний тарифов банка", "уже сформирована !!"}, {;
            " Выход ", " Продолжить "})

      IF KEY = 0 .OR. KEY = 1

         RETURN .T.

      ENDIF

   ENDIF

   KEY := DIALOG(NIL, NIL, {"", "Формировать проводку ?"}, {" Нет ", ;
         " Да "})

   IF KEY <> 2

      RETURN .F.

   ENDIF

   IF EF_S_PR->(DBSEEK("тпл"))

      EF_S_SC->(DBSEEK(EF_S_PR->SC_DT + KOD_VUAH__))
      MEMVAR->SC_D := EF_S_SC->SC_NEW
      EF_S_SC->(DBSEEK(EF_S_PR->SC_KT + KOD_VUAH__))
      MEMVAR->SC_K := EF_S_SC->SC_NEW

   ELSE

      ERROR(NIL, NIL, {"@Внимание !", "В справочнике проводок ", ;
            "не найдена запись с кодом "+CHR(34)+"тпл"+CHR(34)+" "}, {;
            " Выход "})
      RETURN .T.

   ENDIF

   IF EF_SYS->FL_VUD

      DBSELECTAR("ef_doc")
      DBSEEK(DTOS(DAT_OD__) + SC_D + PADR(KOD_B__, 9) + SC_K + PADR(;
            KOD_B__, 9))

      IF RECLOCK(3)

         DBDELETE()

      ENDIF

      DBUNLOCK()

   ENDIF

   DBSELECTAR("ef_jpkpl")

   IF EF_JPKPL->(DBSEEK(DTOS(DAT_OD__)))

   ELSE

      ERROR(NIL, NIL, {"@Внимание !", DTOC(DAT_OD__) + " г.", ;
            "не было приема коммунальных плат. !"}, {" Выход "})

   ENDIF

   DBSELECTAR("ef_jpkpl")

   DO WHILE EF_JPKPL->DAT_BANK == DAT_OD__ .AND. .NOT. EOF()

      MEMVAR->RET := ASCAN(ARR_, {|X|X[1] = EF_JPKPL->KOD_POL})

      IF RET > 0

      ELSE

         EF_S_PPL->(DBSEEK(EF_JPKPL->KOD_POL))
         EF_S_TKP->(DBSEEK(STR(EF_S_PPL->KOD_TAR, 3)))
         EF_S_GPL->(DBSEEK(STR(EF_JPKPL->KOD_VPL, 3)))

         IF  .NOT. EF_S_GPL->PR_KOM

            MEMVAR->AA := STR(EF_S_TKP->PRC_TB, 7, 2)
            MEMVAR->BB := STR(EF_S_TKP->PRC_TKB, 7, 2)
            MEMVAR->CC := STR(EF_S_TKP->PRC_TKP, 7, 2)
            MEMVAR->DD := ALLTRIM(STR(EF_S_TKP->MIN_TB, 7, 2))
            MEMVAR->EE := ALLTRIM(STR(EF_S_TKP->MIN_TKB, 7, 2))
            MEMVAR->FF := ALLTRIM(STR(EF_S_TKP->MIN_TKP, 7, 2))

            IF STR(EF_S_TKP->PRC_TB, 7, 2) == STR(EF_S_TKP->PRC_TKB, 7, 2) ;
                  .AND. STR(EF_S_TKP->PRC_TKB, 7, 2) == STR(EF_S_TKP->;
                  PRC_TKP, 7, 2) .AND. ALLTRIM(STR(EF_S_TKP->MIN_TB, 7, 2)) ;
                  == "0.00" .AND. ALLTRIM(STR(EF_S_TKP->MIN_TKB, 7, 2)) == ;
                  "0.00" .AND. ALLTRIM(STR(EF_S_TKP->MIN_TKP, 7, 2)) == ;
                  "0.00" .AND. .NOT. EF_S_TKP->PR_PR_S

               MEMVAR->FFF := .T.

            ELSE

               MEMVAR->FFF := .F.

            ENDIF

            AADD(ARR_, {EF_JPKPL->KOD_POL, EF_S_PPL->MFO, EF_S_PPL->SC, FFF, ;
                  EF_S_TKP->PRC_TB, EF_S_GPL->PR_TAR})

         ENDIF

      ENDIF

      DBSKIP()

   ENDDO

   ASORT(ARR_, NIL, NIL, {|X,Y|STR(X[1], 3) < STR(Y[1], 3)})
   MEMVAR->NF := ALLTRIM(REP_PATH__) + DTOS(DAT_OD__) + ".vud"
   SET(24, ("") + NF, .F.)
   SET(25, 0)
   SET(20, "PRINTER")
   MEMVAR->RW := PROW()
   DEVPOS(RW + 0, 0)
   DEVOUT(CHR(27)+"!"+CHR(1))
   DEVPOS(RW + 0, 0)
   DEVOUT(CHR(27)+"!"+CHR(4))
   MEMVAR->RW := PROW() + 1
   MEMVAR->RW := MEMVAR->RW + 4
   ++MEMVAR->RW
   DEVPOS(MEMVAR->RW++, 13)
   DEVOUT(;
         "                 Вiдомiсть вiдрахувань тарифiв банка           ")
   DEVPOS(MEMVAR->RW++, 5)
   DEVOUT(("") + DTOC(DAT_OD__) + ;
         " р.                      ( кошти громадян )")
   DEVPOS(MEMVAR->RW++, 0)
   DEVOUT(;
         "====================================================================================";
         )
   DEVPOS(MEMVAR->RW++, 0)
   DEVOUT(;
         "!  N док.  !            Платник                !     Особистий    !      Сума      !";
         )
   DEVPOS(MEMVAR->RW++, 0)
   DEVOUT(;
         "!          !                                   !       рахунок    !                !";
         )
   DEVPOS(MEMVAR->RW++, 0)
   DEVOUT(;
         "!==================================================================================!";
         )
   LEV := 0
   MEMVAR->SS := 99999999999999
   MEMVAR->MM := 999999
   MEMVAR->KK := 9999
   UD_IT := 0
   KWO_IT := 0
   SUM_IT := 0
   MEMVAR->UD_PROV := 0

   FOR I := 1 TO LEN(ARR_)

      IF ARR_[I, 6]

         IF KK <> ARR_[I, 1]

            UD_SC := 0
            SUM_SC := 0
            KWO_SC := 0
            MEMVAR->SS := ARR_[I, 3]
            EF_S_PPL->(DBSEEK(ARR_[I, 1]))
            DEVPOS(MEMVAR->RW++, 0)
            DEVOUT("!" + EF_S_PPL->NAM_POL + "   " + STR(SS, 14))

         ENDIF

         EF_JPKPL->(DBSEEK(DTOS(DAT_OD__)))

         DO WHILE EF_JPKPL->DAT_BANK == DAT_OD__ .AND. .NOT. EOF()

            KOD_V := EF_JPKPL->KOD_PVN

            IF LEN(ALLTRIM(KOD_V)) = 0

               SET(20, "PRINTER")

               IF EF_JPKPL->KOD_POL = ARR_[I, 1]

                  MEMVAR->SUMM := EF_JPKPL->SUMMA
                  DEVPOS(MEMVAR->RW++, 0)
                  DEVOUT("!" + EF_JPKPL->NUM_DOC + "!" + EF_JPKPL->NAM_PLAT +;
                         "     !" + EF_JPKPL->LS_PLAT + "!" + STR(SUMM, 16, ;
                        2) + "!")
                  SUM_IT := SUM_IT + SUMM
                  SUM_SC := SUM_SC + SUMM
                  ++KWO_IT
                  ++KWO_SC

                  IF EF_JPKPL->PR_TAR

                     UD_SC := UD_SC + EF_JPKPL->SUM_KOM

                  ENDIF

               ENDIF

            ENDIF

            DBSELECTAR("ef_jpkpl")
            DBSKIP(1)

         ENDDO

         IF LEN(ARR_) < I + 1 .OR. ARR_[I + 1, 1] <> KK

            DEVPOS(MEMVAR->RW++, 0)
            DEVOUT(;
                  "!==================================================================================!";
                  )

            IF ARR_[I, 4]

               UD_SC := ROUND(SUM_SC * ARR_[I, 5] / 100, 2)
               DEVPOS(MEMVAR->RW++, 0)
               DEVOUT("! Разом по рахунку " + STR(ARR_[I, 3], 14) + ;
                     "                сума прийнята   - " + STR(SUM_SC, 16,;
                      2) + "!")
               DEVPOS(MEMVAR->RW++, 0)
               DEVOUT("! документiв : " + STR(KWO_SC, 4) + "   тариф :" + ;
                     STR(ARR_[I, 5], 7, 2) + ;
                     "%          сума вiдрахування - " + STR(UD_SC, 16, 2) ;
                     + "!")
               DEVPOS(MEMVAR->RW++, 0)
               DEVOUT(;
                     "!                                              для перерахування - ";
                      + STR(SUM_SC - UD_SC, 16, 2) + "!")

            ELSE

               DEVPOS(MEMVAR->RW++, 0)
               DEVOUT("! Разом по рахунку " + STR(ARR_[I, 3], 14) + ;
                     "                сума прийнята   - " + STR(SUM_SC, 16,;
                      2) + "!")
               DEVPOS(MEMVAR->RW++, 0)
               DEVOUT("! документiв : " + STR(KWO_SC, 4) + ;
                     "                            сума вiдрахування - " + ;
                     STR(UD_SC, 16, 2) + "!")
               DEVPOS(MEMVAR->RW++, 0)
               DEVOUT(;
                     "!                                              для перерахування - ";
                      + STR(SUM_SC - UD_SC, 16, 2) + "!")

            ENDIF

            DEVPOS(MEMVAR->RW++, 0)
            DEVOUT(;
                  "!==================================================================================!";
                  )
            MEMVAR->UD_PROV := MEMVAR->UD_PROV + UD_SC
            UD_IT := UD_IT + UD_SC

         ENDIF

      ENDIF

   NEXT

   MEMVAR->RW := MEMVAR->RW + 2
   DEVPOS(MEMVAR->RW++, 0)
   DEVOUT(;
         "!==================================================================================!";
         )
   DEVPOS(MEMVAR->RW++, 0)
   DEVOUT("! Разом документiв : " + STR(KWO_IT, 4) + ;
         "                       сума прийнята    - " + STR(SUM_IT, 16, 2) ;
         + "!")
   DEVPOS(MEMVAR->RW++, 0)
   DEVOUT(;
         "!                                              сума вiдрахування - ";
          + STR(UD_IT, 16, 2) + "!")
   DEVPOS(MEMVAR->RW++, 0)
   DEVOUT(;
         "!                                            для перерахування   - ";
          + STR(SUM_IT - UD_IT, 16, 2) + "!")
   DEVPOS(MEMVAR->RW++, 0)
   DEVOUT(;
         "!==================================================================================!";
         )
   MEMVAR->RW := MEMVAR->RW + 3
   SET(24, "")
   SET(20, "SCREEN")
   EF_SYS->(RECLOCK(3))

   IF UD_IT > 0.001 .AND. EF_DOC->(ADDREC(3))

      MEMVAR->NPP := EF_SYS->N_PP + 1
      EF_DOC->TIP_DOC := "м"
      EF_DOC->VID_OP := "06"
      EF_DOC->NUM_DOC := NPP
      EF_DOC->DAT_KL := DAT_OD__
      EF_DOC->DAT_BANK := DAT_OD__
      EF_DOC->SUMMA := UD_PROV
      EF_S_SC->(DBSEEK(EF_S_PR->SC_DT + KOD_VUAH__))
      EF_DOC->NAM_DT := EF_S_SC->NAM_SC
      EF_DOC->BANK_DT := NAM_B__
      EF_DOC->MFO_DT := KOD_B__
      EF_DOC->SC_DT := EF_S_SC->SC_NEW
      EF_DOC->INDEX_DT := STR(POST_IND__, 6)
      EF_DOC->OKPO_DT := KOD_OKPO__
      EF_S_SC->(DBSEEK(EF_S_PR->SC_KT + KOD_VUAH__))
      EF_DOC->NAM_KT := EF_S_SC->NAM_SC
      EF_DOC->BANK_KT := NAM_B__
      EF_DOC->MFO_KT := KOD_B__
      EF_DOC->SC_KT := EF_S_SC->SC_NEW
      EF_DOC->INDEX_KT := STR(POST_IND__, 6)
      EF_DOC->OKPO_KT := KOD_OKPO__
      EF_DOC->OSN := EF_S_PR->OSN
      EF_DOC->FIO_OP := FIO_OP__
      EF_DOC->KOD_V := KOD_VUAH__
      EF_DOC->FL_INF := .F.
      EF_SYS->N_PP := NPP
      EF_DOC->(DBUNLOCK())

   ENDIF

   EF_SYS->FL_VUD := .T.
   EF_SYS->(DBUNLOCK())
   DBCOMMITAL()
   DIALOG(NIL, NIL, {"", " Ведомость удержаний тарифов и", ;
         "соответствующие проводки ", "сформированы !!!"}, {;
         " ~П~родолжить "})
   DBSELECTAR("ef_jpkpl")
   DBGOTO(REC)

   IF FILE(("") + NF)

      FILEVIEW(1, 0, 23, 79, NF, ;
            "@ Ведомость удержания тарифа с получателей: " + FILENAMES(NF) ;
            + " ", {||PRINTPVD()}, {"~Esc-~выход", "~F2-~печать"})

   ENDIF


RETURN .T.

